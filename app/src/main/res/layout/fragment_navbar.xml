<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dell_5548.eventmusicpestyah_hunyi.NavbarFragment">

    <!-- TODO: Update blank fragment layout -->


    <Button
        android:id="@+id/subscribedButton"
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:padding="15dp"
        android:text="Subscribed" />

    <Button
        android:id="@+id/activesButton"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_toEndOf="@+id/subscribedButton"
        android:padding="15dp"
        android:text="Actives" />


    <Button
        android:id="@+id/archiveButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/activesButton"
        android:padding="15dp"
        android:text="Archived" />

</RelativeLayout>
